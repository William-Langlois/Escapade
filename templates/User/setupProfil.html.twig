        <h1>Vous pouvez planifier un voyage</h1>
        <div class="contenue">


            <div class="input-daterange input-group" id="flight-datepicker">
                <div class="form-item">
                    <label>Depart le</label><span class="fontawesome-calendar"></span>
                    <input
                            class="input-sm form-control" type="text" id="start-date" name="voydatedebut" placeholder="date de départ"
                            data-date-format="DD, MM d" /><span class="date-text date-depart"></span>
                </div>
                <div class="form-item">
                    <label>Retour le </label><span class="fontawesome-calendar"></span>
                    <input
                            class="input-sm form-control" type="text" id="end-date" name="voydatefin" placeholder="date de retour"
                            data-date-format="DD, MM d" /><span class="date-text date-return"></span>
                </div>
            </div>
            <script>
                $(document).ready(function () {
                    let date_input = $('#start-date'); //our date input has the name "date"
                    let container = date_input.parentElement;
                    let options = {
                        format: 'mm/dd/yyyy',
                        container: container,
                        todayHighlight: true,
                        autoclose: true,
                    };
                    date_input.datepicker(options);


                    let date_input2 = $('#end-date'); //our date input has the name "date"
                    let container2 = date_input.parentElement;
                    let options2 = {
                        format: 'mm/dd/yyyy',
                        container: container,
                        todayHighlight: true,
                        autoclose: true,
                    };
                    date_input2.datepicker(options2);
                })
            </script>
            <div>
                <input placeholder="entrez le pays de départ" list="pays" type="text" name="voyfrompays" />
                <input placeholder="entrez la ville de départ" list="villes" type="text" name="voyfromville"  />
            </div>
            <div>
